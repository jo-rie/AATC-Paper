@article{Honarkhah2010,
  title = {Stochastic Simulation of Patterns Using Distance-Based Pattern Modeling},
  author = {Honarkhah, Mehrdad and Caers, Jef},
  date = {2010-07},
  journaltitle = {Mathematical Geosciences},
  volume = {42},
