@article{Perrino1998,
  title = {Intraoperative {{Determination}} of {{Cardiac Output Using Multiplane Transesophageal Echocardiography}}},
  author = {Perrino, Albert~C. and Harris, Stephen~N. and Luther, Martha~A.},
  date = {1998-08-01},
  journaltitle = {Anesthesiology},
  volume = {89},
