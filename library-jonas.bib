@article{Anderson1996,
  title = {A Method for Producing and Evaluating Probabilistic Forecasts from Ensemble Model Integrations},
  author = {Anderson, Jeffrey L.},
  date = {1996},
  journaltitle = {Journal of Climate},
  volume = {9},
